{
  "name": "library-connekto",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "generate:icons": "powershell -ExecutionPolicy Bypass -File generate-icons.ps1",
    "generate:icons:unix": "chmod +x generate-icons.sh && ./generate-icons.sh",
    "create:launcher": "powershell -ExecutionPolicy Bypass -File create-launcher-icon.ps1",
    "build:apk": "powershell -ExecutionPolicy Bypass -File build-apk.ps1",
    "build:aab": "powershell -ExecutionPolicy Bypass -File build-aab.ps1",
    "build:apk:unix": "chmod +x build-apk.sh && ./build-apk.sh",
    "build:aab:unix": "chmod +x build-aab.sh && ./build-aab.sh",
    "build:all": "npm run generate:icons && npm run create:launcher && npm run build:apk && npm run build:aab"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "chart.js": "^4.5.0",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.0",
    "vite": "^7.1.2"
  }
}
