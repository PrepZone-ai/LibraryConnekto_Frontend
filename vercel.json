{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_BASE_URL": "https://ddlsandeep7-libraryconnekto1.hf.space/api/v1"
  },
  "build": {
    "env": {
      "VITE_API_BASE_URL": "https://ddlsandeep7-libraryconnekto1.hf.space/api/v1"
    }
  }
}
